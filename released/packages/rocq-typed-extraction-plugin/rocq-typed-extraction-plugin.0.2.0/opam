  "rocq-elm-extraction" {= version}
  "rocq-rust-extraction" {= version}
]
build: [
  ["bash" "./configure.sh"]
  [make "-j" "%{jobs}%" "-C" "plugin"]
]
install: [
  [make "-C" "plugin" "install"]
]
